<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="quantumworks.droidPluginGen.pluginGen.android"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="quantumworks.droidPluginGen.android.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="quantumworks.droidPluginGen.android.ProjectCreator"
            android:label="@string/title_activity_project_creator"
            android:parentActivityName="quantumworks.droidPluginGen.android.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="quantumworks.droidPluginGen.pluginGen.android.MainActivity" />
        </activity>
        <activity
            android:name="quantumworks.droidPluginGen.android.ProjectSelector"
            android:label="@string/title_activity_project_selector"
            android:parentActivityName="quantumworks.droidPluginGen.android.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="quantumworks.droidPluginGen.pluginGen.android.MainActivity" />
        </activity>
        <activity
            android:name="quantumworks.droidPluginGen.android.ProjectMainView"
            android:label="@string/title_activity_project_main_view" >
        </activity>
    </application>

</manifest>
